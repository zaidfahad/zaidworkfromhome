    "nativescript-local-notifications": "~4.0.1",
    "nativescript-mapbox": "~4.4.1",
    "nativescript-ngx-fonticon": "~4.2.1",
    "nativescript-numeric-keyboard": "~4.3.0",
    "nativescript-plugin-firebase": "~9.1.0",
    "nativescript-snackbar": "~4.1.2",
    "nativescript-social-share": "~1.5.2",
    "nativescript-speech-recognition": "~1.5.0",
